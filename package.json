{
  "name": "sperm-analyzer-ai",
  "version": "1.0.0",
  "description": "Advanced AI-powered sperm analysis application",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "test": "jest",
    "backend": "cd backend && python main.py",
    "dev": "concurrently \"npm run backend\" \"npm run start\"",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:clean": "cd android && ./gradlew clean",
    "build:ios": "react-native run-ios --configuration Release",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "postinstall": "echo 'Installation completed successfully'",
    "clean": "npx react-native clean && cd android && ./gradlew clean && cd ..",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "apk:debug": "npm run bundle:android && npm run build:android:debug",
    "apk:release": "npm run bundle:android && npm run build:android",
    "github:setup": "chmod +x android/gradlew && mkdir -p android/app/src/main/assets"
  },
